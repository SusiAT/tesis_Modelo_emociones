    <!-- http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#ActionTendency -->

    <owl:Class rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#ActionTendency">
        <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#isActionTendencyOf"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Emotion"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State_Cause"/>
        <ace_:CN_pl>ActionTendencies</ace_:CN_pl>
        <ace_:CN_sg>ActionTendency</ace_:CN_sg>
        <rdfs:comment>Emotions have a strong influence on the motivational state of a subject. Emotion theory associates emotions to a small set of so-called action tendencies, e.g. avoidance (relates to fear), rejecting (disgust) etc. Action tendencies can be viewed as a link between the outcome of an appraisal process and actual actions.</rdfs:comment>
        <rdfs:comment>Import From HEO</rdfs:comment>
        <rdfs:label>Action Tendency</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Aesthetic -->

    <owl:Class rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Aesthetic">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Goals/needs"/>
        <ace_:CN_pl>Aesthetics</ace_:CN_pl>
        <ace_:CN_sg>Aesthetic</ace_:CN_sg>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State -->

    <owl:Class rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Mental-State"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#isCausedBy"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State_Cause"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#isDetectedFrom"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State_Recognition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <ace_:CN_pl>Affective_stateses</ace_:CN_pl>
        <ace_:CN_sg>Affective_states</ace_:CN_sg>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State_Cause -->

    <owl:Class rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State_Cause"/>
    


    <!-- http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State_Recognition -->

    <owl:Class rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State_Recognition">
        <rdfs:subClassOf>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.w3.org/2002/07/owl#Thing"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#isSourceToDetect"/>
                        <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:subClassOf>
        <rdfs:comment>Ways to detect or recognize Affective States</rdfs:comment>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Agency -->

    <owl:Class rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Agency">
