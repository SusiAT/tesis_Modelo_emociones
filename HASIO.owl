    <!-- http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#appraisal -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#appraisal">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Appraisal"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">-1</xsd:minExclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxExclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</xsd:maxExclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
        <rdfs:comment>Describe positive or negative values of the properties</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#duration -->

    <owl:DatatypeProperty rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#duration">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Emotion"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Mood"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range rdf:resource="http://org.semanticweb.owlapi/error#Error1"/>
        <rdfs:range>
            <rdfs:Datatype>
                <owl:onDatatype rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                <owl:withRestrictions rdf:parseType="Collection">
                    <rdf:Description>
                        <xsd:minInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</xsd:minInclusive>
                    </rdf:Description>
                    <rdf:Description>
                        <xsd:maxInclusive rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">100</xsd:maxInclusive>
                    </rdf:Description>
                </owl:withRestrictions>
            </rdfs:Datatype>
        </rdfs:range>
    </owl:DatatypeProperty>
    <owl:Axiom>
        <owl:annotatedSource rdf:resource="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#duration"/>
        <owl:annotatedProperty rdf:resource="http://www.w3.org/2000/01/rdf-schema#range"/>
        <owl:annotatedTarget rdf:resource="http://org.semanticweb.owlapi/error#Error1"/>
        <rdfs:comment>Duration  unit = Second</rdfs:comment>
    </owl:Axiom>
    


