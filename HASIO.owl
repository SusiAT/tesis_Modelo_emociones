    <!-- http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#causes -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#causes">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#isCausedBy"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State_Cause"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Emotion_Causes"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Mood_Causes"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Emotion"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Mood"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#hasActionTendency -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#hasActionTendency">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#isActionTendencyOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Emotion"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#ActionTendency"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#hasDuration -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#hasDuration">
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Emotion"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Mood"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#duration"/>
                <owl:someValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
            </owl:Restriction>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#isActionTendencyOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#isActionTendencyOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#ActionTendency"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Emotion"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#isCausedBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#isCausedBy">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Emotion"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Mood"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State_Cause"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Emotion_Causes"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Mood_Causes"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
        <ace_:TV_pl>isCausedBy</ace_:TV_pl>
        <ace_:TV_sg>isCausedBies</ace_:TV_sg>
        <ace_:TV_vbg>isCausedBied</ace_:TV_vbg>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#isDetectedFrom -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#isDetectedFrom">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#isSourceToDetect"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Emotion"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Mood"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Sentiment"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State_Recognition"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Emotion_Recognition"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Mood_Recognition"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Sentiment_Recognition"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#isSourceToDetect -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/rabaa006/ontologies/2014/4/HASO#isSourceToDetect">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Affective_State_Recognition"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Emotion_Recognition"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Mood_Recognition"/>
