    <!-- http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Appraisal -->

    <owl:Class rdf:about="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Appraisal">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/posgrado/ontologies/2020/4/FERO#Emotion_Causes"/>
        <ace_:CN_pl>Appraisals</ace_:CN_pl>
